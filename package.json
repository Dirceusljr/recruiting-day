{
  "name": "backoffice-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite --host",
    "homolog": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "yarn prettier --write",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@sentry/react": "^7.49.0",
    "@sweetalert2/theme-borderless": "^5.0.15",
    "apexcharts": "^3.35.4",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "framer-motion": "^11.1.7",
    "i18next": "^21.8.10",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-flags-select": "^2.2.3",
    "react-google-recaptcha": "^2.1.0",
    "react-i18next": "^11.17.2",
    "react-icons": "^4.4.0",
    "react-number-format": "^5.1.4",
    "react-qr-code": "^2.0.7",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.8",
    "sweetalert2": "^11.7.3",
    "tailwind-merge": "^1.14.0",
    "vite-plugin-add-timestamp": "https://github.com/dersonsena/vite-add-timestamp.git",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@babel/preset-react": "^7.17.12",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@storybook/addon-essentials": "7.3.0",
    "@storybook/addon-interactions": "7.3.0",
    "@storybook/addon-links": "7.3.0",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "7.3.0",
    "@storybook/react": "7.3.0",
    "@storybook/react-vite": "7.3.0",
    "@storybook/testing-library": "0.2.0",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.5.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "postcss": "^8.4.23",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "sass": "^1.53.0",
    "storybook": "7.3.0",
    "tailwindcss": "^3.3.2",
    "vite": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
